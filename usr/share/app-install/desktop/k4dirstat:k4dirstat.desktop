[Desktop Entry]
X-AppInstall-Package=k4dirstat
X-AppInstall-Popcon=99
X-AppInstall-Section=universe

Name=K4DirStat
Exec=k4dirstat %i -qwindowtitle "%c" "%u"
Icon=k4dirstat
Type=Application
X-DocPath=k4dirstat/index.html
GenericName=Directory Statistics
Comment=Directory statistics and disk usage
Terminal=false
MimeType=inode/directory;
Categories=Utility;
Keywords=disk usage, file system, cleanup;

X-Ubuntu-Gettext-Domain=app-install-data
