	    <group>
		<short>Resize Actions</short>
		<option type="bool" name="snapback_windows">
			<short>Snap Windows Back To Original Size</short>
			<long>Snaps windows back to their original size if dragged away from their gridded position.</long>
			<default>true</default>
		</option>
		<option type="bool" name="cycle_sizes">
			<short>Cycle Through Multiple Sizes</short>
			<long>Cycle through multiple different sizes by using the same keyboard shortcut multiple times in a row.</long>
			<default>false</default>
		</option>
		<subgroup>
			<short>Grid Snapback Thresholds</short>
			<option type="int" name="left_edge_threshold">
				<short>Left Edge</short>
				<long>Maximum number of pixels from the left edge a window can be dropped.</long>
				<default>15</default>
				<min>0</min>
				<max>500</max>
			</option>
			<option type="int" name="right_edge_threshold">
				<short>Right Edge</short>
				<long>Maximum number of pixels from the right edge a window can be dropped.</long>
				<default>15</default>
				<min>0</min>
				<max>500</max>
			</option>
			<option type="int" name="top_edge_threshold">
				<short>Top Edge</short>
				<long>Maximum number of pixels from the top edge a window can be dropped.</long>
				<default>20</default>
				<min>0</min>
				<max>500</max>
			</option>
			<option type="int" name="bottom_edge_threshold">
				<short>Bottom Edge</short>
				<long>Maximum number of pixels from the bottom edge a window can be dropped.</long>
				<default>5</default>
				<min>0</min>
				<max>500</max>
			</option>
		</subgroup>
		<subgroup>
			<short>Grid Snapoff Threshold</short>
			<option type="int" name="snapoff_threshold">
				<short>General Size</short>
				<long>The pixels to drag until a grid-resized window snaps off.</long>
				<default>50</default>
				<min>0</min>
				<max>500</max>
			</option>
		</subgroup>
